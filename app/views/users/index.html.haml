.page-header
  %h1=t '.title', :default => 'User'
%table.table.table-striped
  %thead
    %tr
      %th Email
      %th UserName
      %th Created At
  %tbody
    - @users.each do |user|
      %tr
        %td= link_to(user.email, admin_user_path(user))
        %td= user.username
        %td=l user.created_at

= paginate @users