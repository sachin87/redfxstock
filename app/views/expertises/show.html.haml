.page-header
  %h1=t '.title', :default => 'Expertise'

%p
  %strong Name:
  = @expertise.name

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), expertises_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_expertise_path(@expertise), :class => 'btn'
