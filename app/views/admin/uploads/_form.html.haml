= simple_form_for [:admin, @resource], :html => { :class => 'form-horizontal' } do |f|
  = f.input :name
  = f.association :gallery
  = f.input :image
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), admin_uploads_path, :class => 'btn'
